<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="ir_exp_shopinvader_variant" model="ir.exports">
        <field name="name">ShopInvader Product Export</field>
        <field name="resource">shopinvader.variant</field>
    </record>

    <record id="ir_exp_shopinvader_variant_record_id" model="ir.exports.line">
        <field name="name">record_id</field>
        <field name="target">record_id:id</field>
        <field name="instance_method_name">_jsonify_m2o_to_id</field>
        <field name="export_id" ref="ir_exp_shopinvader_variant" />
    </record>

    <record id="ir_exp_shopinvader_variant_model_name" model="ir.exports.line">
        <field name="name">shopinvader_product_id/shopinvader_display_name</field>
        <field
            name="target"
        >shopinvader_product_id:model/shopinvader_display_name:name</field>
        <field name="export_id" ref="ir_exp_shopinvader_variant" />
    </record>

    <record id="ir_exp_shopinvader_variant_url_key" model="ir.exports.line">
        <field name="name">url_key</field>
        <field name="export_id" ref="ir_exp_shopinvader_variant" />
    </record>

    <record id="ir_exp_shopinvader_variant_redirect_url_key" model="ir.exports.line">
        <field name="name">redirect_url_key</field>
        <field name="export_id" ref="ir_exp_shopinvader_variant" />
    </record>

    <record id="ir_exp_shopinvader_variant_main" model="ir.exports.line">
        <field name="name">main</field>
        <field name="export_id" ref="ir_exp_shopinvader_variant" />
    </record>

    <record id="ir_exp_shopinvader_short_description" model="ir.exports.line">
        <field name="name">short_description</field>
        <field name="export_id" ref="ir_exp_shopinvader_variant" />
    </record>

    <record id="ir_exp_shopinvader_description" model="ir.exports.line">
        <field name="name">description</field>
        <field name="export_id" ref="ir_exp_shopinvader_variant" />
    </record>

    <record id="ir_exp_shopinvader_name" model="ir.exports.line">
        <field name="name">full_name</field>
        <field name="target">full_name:name</field>
        <field name="export_id" ref="ir_exp_shopinvader_variant" />
    </record>

    <record id="ir_exp_shopinvader_short_name" model="ir.exports.line">
