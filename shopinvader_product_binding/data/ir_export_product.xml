    </record>

    <record id="ir_exp_shopinvader_variant_categs_id" model="ir.exports.line">
        <field name="name">shopinvader_categ_ids/record_id</field>
        <field name="target">shopinvader_categ_ids:categories/record_id:id</field>
        <field name="instance_method_name">_jsonify_m2o_to_id</field>
        <field name="export_id" ref="ir_exp_shopinvader_variant" />
