         <!-- Create a service product  adaptoo Colissimo International Zone 4 -->
            <!-- max 30 kg,-->

        <record id="product_product_delivery_inter_zone4" model="product.product">
            <field name="name">Colissimo International Zone 4</field>
            <field name="default_code">colissimo_4_30</field>
            <field name="type">service</field>
            <field name="categ_id" ref="product.product_category_6"/>
            <field name="standard_price"></field>
            <field name="list_price"></field>
            <field name="image" type="base64" file="delivery/static/img/product_product_delivery-image.jpg"/>
        </record>

         <!-- Carrier -->
        <record id="delivery_carrier_inter_zone4" model="delivery.carrier">
            <field name="name">Colissimo International Zone 4</field>
            <field name="normal_price"></field>
            <field name="partner_id" ref="res_partner_74"/>
            <field name="product_id" ref="product_product_delivery_inter_zone4"/>
            <field name="use_detailed_pricelist" eval="True"/>
        </record>

