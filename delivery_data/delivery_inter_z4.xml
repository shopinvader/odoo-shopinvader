        <record id="delivery_grid_inter_zone4" model="delivery.grid">
            <field name="name">Pricing Colissimo International Zone 4</field>
            <field name="carrier_id" ref="delivery_carrier_inter_zone4"/>
            <field model="res.country" name="country_ids" search="[('code','ilike','AT')]"/>
        </record>

