    <record id="ir_exp_shopinvader_variant_brand_url_key" model="ir.exports.line">
        <field name="name">shopinvader_brand_id/url_key</field>
        <field name="target">shopinvader_brand_id:brand/url_key</field>
        <field
            name="export_id"
            ref="shopinvader_product_binding.ir_exp_shopinvader_variant"
        />
