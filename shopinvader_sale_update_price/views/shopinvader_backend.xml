<odoo>

    <record model="ir.ui.view" id="shopinvader_backend_form_view">
        <field
            name="name"
        >shopinvader.backend.form (in shopinvader_sale_update_price)</field>
        <field name="model">shopinvader.backend</field>
        <field
            name="inherit_id"
            ref="shopinvader_v1_product.shopinvader_backend_view_form"
        />
        <field name="arch" type="xml">
            <page name='developer' position='inside'>
                <group name="prices" string="Prices">
                    <button
                        name="launch_sale_price_update"
                        string="Launch Prices Update"
                        type="object"
                    />
                </group>
            </page>
        </field>
    </record>



</odoo>
