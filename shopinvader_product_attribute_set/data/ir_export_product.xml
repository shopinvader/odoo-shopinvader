<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="ir_exp_shopinvader_attributes" model="ir.exports.line">
        <field name="name">attributes</field>
        <field
            name="export_id"
            ref="shopinvader_product_binding.ir_exp_shopinvader_variant"
        />
    </record>

    <record id="ir_exp_shopinvader_structured" model="ir.exports.line">
        <field name="name">structured_attributes</field>
        <field
            name="export_id"
            ref="shopinvader_product_binding.ir_exp_shopinvader_variant"
        />
