        <field name="target">variant_rating_ids:ratings/feedback</field>
        <field name="export_id" ref="shopinvader.ir_exp_shopinvader_variant" />
    </record>

    <record id="ir_exp_shopinvader_variant_rating" model="ir.exports.line">
        <field name="name">variant_rating_ids/rating</field>
        <field name="target">variant_rating_ids:ratings/rating</field>
        <field name="export_id" ref="shopinvader.ir_exp_shopinvader_variant" />
    </record>

    <record id="ir_exp_shopinvader_variant_rating_id" model="ir.exports.line">
        <field name="name">variant_rating_ids/id</field>
        <field name="target">variant_rating_ids:ratings/id</field>
        <field name="export_id" ref="shopinvader.ir_exp_shopinvader_variant" />
    </record>

    <record id="ir_exp_shopinvader_variant_rating_date" model="ir.exports.line">
        <field name="name">variant_rating_ids/write_date</field>
        <field name="target">variant_rating_ids:ratings/write_date:date</field>
        <field name="export_id" ref="shopinvader.ir_exp_shopinvader_variant" />
    </record>

